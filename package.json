{
  "name": "{{projectName}}",
  "version": "{{version}}",
  "author": "{{author}}",
  "description": "{{description}}",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:test": "env-cmd -f .env.test vue-cli-service build && env-cmd -f .env.test node ./version/update.js",
    "build:prod": "env-cmd -f .env.production vue-cli-service build && env-cmd -f .env.production node ./version/update.js",
    "test:unit": "vue-cli-service test:unit",
    "lint:ts": "vue-cli-service lint",
    "lint:stylelint": "stylelint {src,mock}/**/*.{css,less,sass,scss,vue} --fix",
    "lint:prettier": "prettier --write  \"{src,mock}/**/*.{js,ts,json,tsx,css,less,scss,jsx,html,md,vue}\"",
    "lint": "npm run lint:ts && npm run lint:stylelint && npm run lint:prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.8",
    "axios": "^0.27.2",
    "axios-retry": "~3.1.9",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.8.3",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.9.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^27.0.1",
    "@types/js-base64": "^2.3.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.11.9",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-jest": "^27.0.6",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.3",
    "jest": "^27.0.5",
    "less": "^4.0.0",
    "less-loader": "^6.0.0",
    "lint-staged": "^11.1.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.4.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue-template-compiler": "^2.6.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": [
      "npm run lint",
      "git add",
      "git commit"
    ]
  }
}
